{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "redirects": [
    {
      "source": "/:path*",      
      "has": [
        {
          "type": "host",
          "value": "www.playsnow.net"   
        }
      ],
      "destination": "https://playsnow.net/:path*",
      "permanent": true               
    }
  ]
}
